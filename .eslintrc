{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": ["@react-native-community","plugin:cypress/recommended"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ],
    "jsx-quotes": ["error", "prefer-single"],
    "no-shadow": "off",
    "@typescript-eslint/no-shadow": ["error"]
  },
  "ignorePatterns": ["dist/*"],
  "env": {
    "browser": true
  }
}
